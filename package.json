{
  "name": "safe-types-combinators",
  "version": "1.1.2",
  "description": "result-validators",
  "main": "lib/validators.js",
  "module": "lib/validators.js",
  "types": "lib/validators.d.js",
  "scripts": {
    "prebuild": "rm -Rf lib esm",
    "build:docs": "typedoc --options typedoc.js",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.json",
    "build": "npm run build:cjs && npm run build:esm",
    "prepublishOnly": "npm run build",
    "prerelease": "npm run test:cov",
    "release": "standard-version",
    "postrelease": "git push --follow-tags origin master && npm publish",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test:v": "jest --verbose",
    "test:cov": "jest --verbose --coverage",
    "test": "npm run test:cov"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "files": [
    "lib",
    "esm",
    "src"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js|ts)$"
  },
  "dependencies": {
    "safe-types": "^4.3.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "@types/node": "^10.9.4",
    "jest": "^23.6.0",
    "standard-version": "^4.4.0",
    "ts-jest": "^23.1.4",
    "ts-node": "^7.0.1",
    "typedoc": "^0.12.0",
    "typescript": "^3.0.3"
  }
}
