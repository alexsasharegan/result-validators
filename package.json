{
  "name": "result-validators",
  "version": "1.0.0",
  "description": "result-validators",
  "main": "lib/validators.js",
  "types": "lib/validators.d.js",
  "scripts": {
    "prebuild": "rm -Rf lib",
    "build:docs": "typedoc --options typedoc.js",
    "build": "tsc && cp flow-typed/validators.js lib/validators.js.flow",
    "prepublishOnly": "npm run build",
    "prerelease": "npm run test:cov",
    "release": "standard-version",
    "postrelease": "git push --follow-tags origin master && npm publish",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test:v": "jest --verbose",
    "test:cov": "jest --verbose --coverage",
    "test": "npm run test:cov"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js|ts)$"
  },
  "dependencies": {
    "safe-types": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^23.1.5",
    "@types/node": "^10.5.2",
    "jest": "^23.4.0",
    "standard-version": "^4.4.0",
    "ts-jest": "^23.0.0",
    "ts-node": "^7.0.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.9.2"
  }
}
